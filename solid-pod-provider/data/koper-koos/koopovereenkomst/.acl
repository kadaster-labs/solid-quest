@prefix : <#>.
@prefix acl: <http://www.w3.org/ns/auth/acl#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix koo: <./>.
@prefix c: <../profile/card#>.

:AppendRead
    a acl:Authorization;
    acl:accessTo koo:;
    acl:agentClass acl:AuthenticatedAgent;
    acl:mode acl:Append, acl:Read.
:ControlReadWrite
    a acl:Authorization;
    acl:accessTo koo:;
    acl:agent c:me, <mailto:koos@world.com>;
    acl:mode acl:Control, acl:Read, acl:Write.
:ControlReadWriteDefault
    a acl:Authorization;
    acl:agent c:me, <mailto:koos@world.com>;
    acl:default koo:;
    acl:mode acl:Control, acl:Read, acl:Write.
:Read
    a acl:Authorization;
    acl:accessTo koo:;
    acl:agentClass foaf:Agent;
    acl:mode acl:Read.
